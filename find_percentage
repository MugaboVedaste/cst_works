#include <stdio.h>

int main() {
    // Declare variables for marks in five subjects
    int subject1, subject2, subject3, subject4, subject5;
    float overallPercentage;

    // Input marks for five subjects
    printf("Enter marks for five subjects (out of 100):\n");
    
    // Subject 1
    printf("Subject 1: ");
    scanf("%d", &subject1);

    // Check for valid input
    if (subject1 < 0 || subject1 > 100) {
        printf("Invalid marks for Subject 1\n");
        return 1; // Exit program with an error code
    }

    // Subject 2
    printf("Subject 2: ");
    scanf("%d", &subject2);

    // Check for valid input
    if (subject2 < 0 || subject2 > 100) {
        printf("Invalid marks for Subject 2\n");
        return 1; // Exit program with an error code
    }

    // Subject 3
    printf("Subject 3: ");
    scanf("%d", &subject3);

    // Check for valid input
    if (subject3 < 0 || subject3 > 100) {
        printf("Invalid marks for Subject 3\n");
        return 1; // Exit program with an error code
    }

    // Subject 4
    printf("Subject 4: ");
    scanf("%d", &subject4);

    // Check for valid input
    if (subject4 < 0 || subject4 > 100) {
        printf("Invalid marks for Subject 4\n");
        return 1; // Exit program with an error code
    }

    // Subject 5
    printf("Subject 5: ");
    scanf("%d", &subject5);

    // Check for valid input
    if (subject5 < 0 || subject5 > 100) {
        printf("Invalid marks for Subject 5\n");
        return 1; // Exit program with an error code
    }

    // Calculate overall percentage
    overallPercentage = (subject1 + subject2 + subject3 + subject4 + subject5) / 5.0;

    // Determine overall grade based on percentage
    printf("\nOverall Percentage: %.2f%%\n", overallPercentage);

    // Determine overall grade based on percentage
    if (overallPercentage >= 90 && overallPercentage <= 100) {
        printf("Overall Grade: A\n");
    } else if (overallPercentage >= 80 && overallPercentage < 90) {
        printf("Overall Grade: B\n");
    } else if (overallPercentage >= 70 && overallPercentage < 80) {
        printf("Overall Grade: C\n");
    } else if (overallPercentage >= 60 && overallPercentage < 70) {
        printf("Overall Grade: D\n");
    } else if (overallPercentage >= 50 && overallPercentage < 60) {
        printf("Overall Grade: E\n");
    } else if (overallPercentage < 50) {
        printf("Overall Grade: F\n");
    } else {
        printf("Invalid overall percentage\n");
    }

    // Print individual subject grades
    printf("\nSubject 1 Grade: ");
    if (subject1 >= 90 && subject1 <= 100) {
        printf("A\n");
    } else if (subject1 >= 80 && subject1 < 90) {
        printf("B\n");
    } else if (subject1 >= 70 && subject1 < 80) {
        printf("C\n");
    } else if (subject1 >= 60 && subject1 < 70) {
        printf("D\n");
    } else if (subject1 >= 50 && subject1 < 60) {
        printf("E\n");
    } else if (subject1 < 50) {
        printf("F\n");
    } else {
        printf("Invalid grade\n");
    }

    printf("Subject 2 Grade: ");
    // Similar logic for subject2...
    if (subject2 >= 90 && subject2 <= 100) {
        printf("A\n");
    } else if (subject2 >= 80 && subject2 < 90) {
        printf("B\n");
    } else if (subject2 >= 70 && subject2 < 80) {
        printf("C\n");
    } else if (subject2 >= 60 && subject2 < 70) {
        printf("D\n");
    } else if (subject2 >= 50 && subject2 < 60) {
        printf("E\n");
    } else if (subject2 < 50) {
        printf("F\n");
    } else {
        printf("Invalid grade\n");
    }

    printf("Subject 3 Grade: ");
    // Similar logic for subject3...
    if (subject3 >= 90 && subject3 <= 100) {
        printf("A\n");
    } else if (subject3 >= 80 && subject3 < 90) {
        printf("B\n");
    } else if (subject3 >= 70 && subject3 < 80) {
        printf("C\n");
    } else if (subject3 >= 60 && subject3 < 70) {
        printf("D\n");
    } else if (subject3 >= 50 && subject3 < 60) {
        printf("E\n");
    } else if (subject3 < 50) {
        printf("F\n");
    } else {
        printf("Invalid grade\n");
    }

    printf("Subject 4 Grade: ");
    // Similar logic for subject4...
    if (subject4 >= 90 && subject4 <= 100) {
        printf("A\n");
    } else if (subject4 >= 80 && subject4 < 90) {
        printf("B\n");
    } else if (subject4 >= 70 && subject4 < 80) {
        printf("C\n");
    } else if (subject4 >= 60 && subject4 < 70) {
        printf("D\n");
    } else if (subject4 >= 50 && subject4 < 60) {
        printf("E\n");
    } else if (subject4 < 50) {
        printf("F\n");
    } else {
        printf("Invalid grade\n");
    }

    printf("Subject 5 Grade: ");
    // Similar logic for subject5...
    if (subject5 >= 90 && subject5 <= 100) {
        printf("A\n");
    } else if (subject5 >= 80 && subject5 < 90) {
        printf("B\n");
    } else if (subject5 >= 70 && subject5 < 80) {
        printf("C\n");
    } else if (subject5 >= 60 && subject5 < 70) {
        printf("D\n");
    } else if (subject5 >= 50 && subject5 < 60) {
        printf("E\n");
    } else if (subject5 < 50) {
        printf("F\n");
    } else {
        printf("Invalid grade\n");
    }

    return 0;
}
